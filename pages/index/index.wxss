/*新加的  */
.test {
  border: 1px solid black;
}
/*通用的样式  */
.centered {
  width: 90%;
  margin: 0 auto;
}
#id-view-all {
  background: url('data:image/gif;base64,')
}
/*第一部分顶部样式  */
#id-view-top {
  height: 100px;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px dashed #DDDDDD;
}
#id-view-top .left{

}
#id-view-top .left1 {
  font-size: 22px;
  margin-bottom: 10px; 
}
#id-view-top .left2 {
  font-size: 14px;
  color: #969696;
}
#id-view-top .right1 {
  width: 50px;
  height: 50px;
  border-radius: 50%;
}
/*第二部分中间的内容部分  */
#id-view-center {
  display: flex;
  flex-direction: column;
}
#id-view-center .view-li{
  display: flex;
  flex-direction: row;
  height: 50px;
  align-items: center;
  font-size: 14px;
  position: relative;
}
#id-view-center .images-css {
  width: 20px;
  height: 20px;
  margin-right: 15px;
}
.image-arrow {
  width: 20px;
  height: 25px;
  position: absolute;
  right: 5px;
}
/*第三部分菜谱展示  */
/* .circle  { 
    -webkit-border-radius: 50em; 
    -moz-border-radius: 50em; 
    border-radius: 50em; 
} */
.image-wrap { 
    -webkit-box-shadow: inset 0 0 4px rgba(0,0,0,1), inset 0 2px 1px rgba(255,255,255,.5), inset 0 -9px 2px rgba(0,0,0,.6), inset 0 -12px 2px rgba(255,255,255,.3); 
    -moz-box-shadow: inset 0 0 4px rgba(0,0,0,1), inset 0 2px 1px rgba(255,255,255,.5), inset 0 -9px 2px rgba(0,0,0,.6), inset 0 -12px 2px rgba(255,255,255,.3); 
    box-shadow: inset 0 0 4px rgba(0,0,0,1), inset 0 2px 1px rgba(255,255,255,.5), inset 0 -9px 2px rgba(0,0,0,.6), inset 0 -12px 2px rgba(255,255,255,.3); 
    -webkit-border-radius: 20px; 
    -moz-border-radius: 20px; 
    border-radius: 20px; 
}
